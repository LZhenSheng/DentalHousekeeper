<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="588.0" prefWidth="415.0" styleClass="panel" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.RegisterController">
   <children>
      <Label layoutX="180.0" layoutY="15.0" prefHeight="57.0" prefWidth="107.0" styleClass="panel-title" text="欢迎注册">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="86.0" layoutY="72.0" prefHeight="44.0" prefWidth="249.0" promptText="医院名称" style="-fx-alignment: center; -fx-background-radius: 5;" />
      <TextField fx:id="phone" layoutX="86.0" layoutY="121.0" prefHeight="44.0" prefWidth="249.0" promptText="手机号" style="-fx-alignment: center; -fx-background-radius: 5;" />
      <TextField fx:id="code" layoutX="86.0" layoutY="173.0" prefHeight="44.0" prefWidth="143.0" promptText="验证码" style="-fx-alignment: center; -fx-background-radius: 5;" />
      <TextField fx:id="password" layoutX="84.0" layoutY="277.0" prefHeight="44.0" prefWidth="249.0" promptText="密码" style="-fx-alignment: center; -fx-background-radius: 5;" />
      <TextField fx:id="province" layoutX="83.0" layoutY="333.0" prefHeight="44.0" prefWidth="249.0" promptText="省" style="-fx-alignment: center; -fx-background-radius: 5;" />
      <TextField fx:id="city" layoutX="83.0" layoutY="391.0" prefHeight="44.0" prefWidth="249.0" promptText="市" style="-fx-alignment: center; -fx-background-radius: 5;" />
      <TextField fx:id="area" layoutX="83.0" layoutY="450.0" prefHeight="44.0" prefWidth="249.0" promptText="区" style="-fx-alignment: center; -fx-background-radius: 5;" />
      <TextField fx:id="pid" layoutX="85.0" layoutY="227.0" prefHeight="44.0" prefWidth="249.0" promptText="PID（2088开头商户ID）" style="-fx-alignment: center; -fx-background-radius: 5;" />
      <JFXButton layoutX="249.0" layoutY="173.0" onAction="#sendSMS" prefHeight="44.0" prefWidth="86.0" text="获取">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-info" />
         </styleClass>
      </JFXButton>
      <JFXButton layoutX="81.0" layoutY="514.0" onAction="#register" prefHeight="44.0" prefWidth="256.0" text="注册">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-info" />
         </styleClass>
      </JFXButton>
   </children>
</AnchorPane>
